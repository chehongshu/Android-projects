<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/feijicanshu_Layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:screenOrientation="landscape"
    android:background="#ffffff">

    <ImageView
        android:layout_width="156dp"
        android:layout_height="match_parent"
        android:background="@drawable/gaodu"
        android:id="@+id/imageView6" />

    <Button
        android:id="@+id/kongzhi_button"
        android:layout_width="100dip"
        android:layout_height="100dip"
        android:layout_marginLeft="420dp"
        android:layout_marginTop="170dp"
        android:background="@drawable/kongzhi" />

    <Button
        android:id="@+id/suoding_button"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_marginLeft="320dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/suoding" />

    <Button
        android:id="@+id/jiangluo_button"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_marginLeft="250dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/jiangluo" />

    <Button
        android:id="@+id/yonghu_button"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_marginLeft="180dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/yonghu" />

    <RelativeLayout
        android:layout_width="150dp"
        android:layout_height="100dp"
        android:layout_alignEnd="@+id/suoding_button"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/yonghu_button"
        android:layout_marginTop="42dp" >

        <include layout="@layout/parameter_sheet" />
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="1000dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/yonghu_button"
        android:layout_alignTop="@+id/suoding_button"
        android:background="@drawable/youxiajiao" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/yonghu_button"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="22dp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/kongzhi_button"
        android:layout_marginEnd="38dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/feiji_xia" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/feiji_you"
        android:layout_marginBottom="11dp"
        android:layout_above="@+id/imageView4"
        android:layout_alignStart="@+id/suoding_button"
        android:layout_marginStart="12dp" />

</RelativeLayout>