<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.che.maptest.map_Activity">


    <com.amap.api.maps.MapView
        android:id="@+id/map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        >

        <Button
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:background="@drawable/jiangluo"
            android:id="@+id/jiangluo_button"
            android:layout_marginLeft="340dp"
            android:layout_marginTop="180dp"  />
        <Button
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:background="@drawable/yonghu"
            android:id="@+id/yonghu_button"
            android:layout_marginLeft="280dp"
            android:layout_marginTop="180dp"  />

        <Button
            android:layout_width="100dip"
            android:layout_height="86dp"
            android:background="@drawable/kongzhi"
            android:id="@+id/kongzhi_button"
            android:layout_marginLeft="470dp"
            android:layout_marginTop="150dp"  />

        <Button
            android:layout_width="50dip"
            android:layout_height="50dip"
            android:background="@drawable/suoding"
            android:id="@+id/suoding_button"
            android:layout_marginLeft="400dp"
            android:layout_marginTop="180dp"  />

        <ImageView
            android:layout_width="156dp"
            android:layout_height="224dp"
            android:background="@drawable/gaodu"
            android:id="@+id/imageView"
            android:layout_marginLeft="35dp"
            android:layout_marginTop="30dp"/>
    </com.amap.api.maps.MapView>


</RelativeLayout>

